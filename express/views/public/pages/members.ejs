<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Members | Twitch Mod Squad</title>

<%- include("../partials/head") %>

</head>
<body>

<%- include("../partials/header") %>

    <main>
<%- include("../partials/heading-section") %>
        <section class="solid">
            <div class="wrapper">
                <h2>
                    Members
                </h2>
                <div class="user-container wrap">
                    <% members.forEach(user => { %>
                    <%- include("../partials/user", {user: user, comma: comma}) %>
                    <% }) %>
                </div>
            </div>
        </section>
    </main>

<%- include("../partials/footer") %>

<%- include("../partials/javascript") %>
</body>
</html>
